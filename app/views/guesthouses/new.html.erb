<%= form_for @guesthouse do |f| %>
  <fieldset class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </fieldset>


  <fieldset class="form-group">
    <%= f.label :address, 'Address' %>
    <%= f.text_field :address, class: "form-control" %>
  </fieldset>

  <%= f.submit 'Add!', class: 'btn btn-primary' %>
<% end %>
